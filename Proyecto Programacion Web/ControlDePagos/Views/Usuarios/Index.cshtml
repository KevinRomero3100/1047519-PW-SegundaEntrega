@model IEnumerable<LibreriaPagos.Models.Usuario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Codigo de empleado")
            </th>
            <th>
                @Html.DisplayName("Nombre de Usuario")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmpleadoIdEmpleadoNavigation.CodigoPersonal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreDeUsuario)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.IdUsuario }) |
                    @Html.ActionLink("Details", "Details", new { id=item.IdUsuario }) |
                    @Html.ActionLink("Delete", "Delete", new { id=item.IdUsuario })
            </td>
        </tr>
}
    </tbody>
</table>
